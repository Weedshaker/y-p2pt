{
  "name": "y-p2pt",
  "version": "0.0.1",
  "description": "p2pt Connector for Yjs",
  "scripts": {
    "fix": "standard --fix",
    "serve": "five-server --port=3000 --host=localhost --no-css-inject",
    "build": "rm -rf ./src/es/y-p2pt.js && rm -rf ./src/es/p2pt.js && rollup -c -w"
  },
  "author": "weedshaker@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "five-server": "*",
    "rollup": "*",
    "@rollup/plugin-node-resolve": "*",
    "@rollup/plugin-commonjs": "*",
    "standard": "*"
  },
  "standard": {
    "ignore": [
      "/src/es/dependencies/",
      "/src/yjs/",
      "/src/y-*/"
    ]
  }
}
